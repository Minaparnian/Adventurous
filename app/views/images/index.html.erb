<nav>
  <%= link_to 'Home', images_path %>

  <% if @current_user.present? %>
    <%= link_to "Edit profile", edit_user_path(@current_user), :class => "home-nav"%>
    <%= link_to "Sign out #{ @current_user.name }", login_path, :method => 'delete', :class => "home-nav" %>
  <% else %>
    <%= link_to 'Sign up', new_user_path %>
    <!-- <%= link_to 'Sign in', login_path %> -->
  <% end %>
</nav>
<h1>Welcome to images page</h1>

<div class="all_img_container">
<% @images.each do |i|%>

  <a href="/images/<%= i.id %>">
<%= image_tag i.image, :class => "all_img"%>
</a>


<% end %>
</div>

<%= link_to "Add Image", new_image_path %>
