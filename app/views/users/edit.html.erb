<div class="sign-container">

<h1>Edit user</h1>
<div class = "table">

<%= form_for @user do |f| %>
<fieldset>
<%= f.label :email%>
<%= f.email_field :email, :placeholder => 'Email' %>
</fieldset>

<div class="submit">
<label></label>
<fieldset>
<%= f.submit "Edit", :class => "button" %>
</fieldset>

<% end %>
</div><!--submit-->

